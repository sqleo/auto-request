      "/shop/visit/statistics": {
        "get": {
          "tags": [
            "门店模块"
          ],
          "summary": "人员拜访统计",
          "description": "",
          "parameters": [
            {
              "name": "year",
              "in": "query",
              "required": true,
              "description": "年",
              "type": "string"
            },
            {
              "name": "month",
              "in": "query",
              "required": true,
              "description": "月",
              "type": "string"
            },
            {
              "name": "businessUnitId",
              "in": "query",
              "required": false,
              "description": "事业部ID",
              "type": "string"
            },
            {
              "name": "salesAreaId",
              "in": "query",
              "required": false,
              "description": "销售大区ID",
              "type": "string"
            },
            {
              "name": "salesDeptId",
              "in": "query",
              "required": false,
              "description": "销售省区ID",
              "type": "string"
            },
            {
              "name": "salesGroupId",
              "in": "query",
              "required": false,
              "description": "销售城市群ID",
              "type": "string"
            },
            {
              "name": "salesOfficeId",
              "in": "query",
              "required": false,
              "description": "销售区县ID",
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "object",
                "title": "empty object",
                "properties": {
                  "year": {
                    "type": "integer",
                    "description": "年"
                  },
                  "month": {
                    "type": "integer",
                    "description": "月"
                  },
                  "orgId": {
                    "type": "integer",
                    "description": "组织ID"
                  },
                  "orgName": {
                    "type": "string",
                    "description": "组织名称"
                  },
                  "orgType": {
                    "type": "integer",
                    "description": "组织类型"
                  },
                  "subOrg": {
                    "type": "object",
                    "properties": {},
                    "description": "子组织"
                  },
                  "statisticsList": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "salesOfficeId": {
                          "type": "integer",
                          "description": "销售区县ID"
                        },
                        "salesOfficeName": {
                          "type": "string",
                          "description": "销售区县"
                        },
                        "userId": {
                          "type": "integer",
                          "description": "用户ID"
                        },
                        "userName": {
                          "type": "string",
                          "description": "用户名称"
                        },
                        "visitNumber": {
                          "type": "integer",
                          "description": "计划拜访次数"
                        },
                        "actualVisitNumber": {
                          "type": "integer",
                          "description": "实际拜访次数"
                        },
                        "finishPercent": {
                          "type": "string",
                          "description": "本月达成率"
                        }
                      },
                      "required": [
                        "salesOfficeId",
                        "salesOfficeName",
                        "userId",
                        "userName",
                        "visitNumber",
                        "actualVisitNumber",
                        "finishPercent"
                      ],
                      "description": "拜访统计"
                    },
                    "description": "当前组织的拜访统计列表"
                  }
                },
                "required": [
                  "year",
                  "orgName",
                  "subOrg",
                  "statisticsList",
                  "orgId",
                  "month",
