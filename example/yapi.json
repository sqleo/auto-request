                    "description": "纬度",
                    "minimum": 0
                  },
                  "contact": {
                    "type": "string",
                    "description": "联系人"
                  },
                  "mobile": {
                    "type": "string",
                    "description": "联系人手机"
                  },
                  "phone": {
                    "type": "string",
                    "description": "联系人座机"
                  },
                  "description": {
                    "type": "string",
                    "description": "描述"
                  },
                  "status": {
                    "type": "integer",
                    "description": "状态",
                    "enum": [
                      10,
                      20
                    ]
                  }
                },
                "required": [
                  "id",
                  "name",
                  "direct",
                  "category",
                  "provinceId",
                  "province",
                  "cityId",
                  "city",
                  "countyId",
                  "county",
                  "address",
                  "lng",
                  "lat",
                  "description",
                  "mobile",
                  "contact",
                  "status",
                  "phone",
